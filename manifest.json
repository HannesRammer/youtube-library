{
  "name": "youtube library",
  "description": "Manage played youtube songs like songs google play music",
  "version": "0.0.1",
  "version_name": "0.0.1 beta",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "youtube library"
  },
  "oauth2": {
    "client_id": "477580242585-65stuf3qlsf4i17brijqqem1jij7ht0n.apps.googleusercontent.com",
    "scopes":["https://www.googleapis.com/auth/youtube"]
  },
  "permissions": [
    "identity",
    
    "storage"

  ],
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js" : ["json_inject.js"]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "manifest_version": 2
}
